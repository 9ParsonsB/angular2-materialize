{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angular2-materialize"
  },
  "apps": [
    {
      "root": ".",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "sample/index.html",
      "main": "sample/main.ts",
      "polyfills": "sample/polyfills.ts",
      "test": "sample/test.ts",
      "tsconfig": "sample/tsconfig.app.json",
      "testTsconfig": "sample/tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "sample/styles.css",
        "./node_modules/materialize-css/dist/css/materialize.css"
      ],
      "scripts": [
        "./node_modules/jquery/dist/jquery.js",
        "./node_modules/hammerjs/hammer.js",
        "./node_modules/materialize-css/dist/js/materialize.js"
      ],
      "environmentSource": "sample/environments/environment.ts",
      "environments": {
        "dev": "sample/environments/environment.ts",
        "prod": "sample/environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
