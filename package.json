{
  "name": "angular2-materialize",
  "version": "15.14.0",
  "description": "Angular 2 support for Materialize CSS framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/9parsonsb/angular2-materialize.git"
  },
  "homepage": "https://github.com/9parsonsb/angular2-materialize#readme",
  "bugs": {
    "url": "https://github.com/9parsonsb/angular2-materialize/issues"
  },
  "files": [
    "lib_commonjs",
    "lib_esmodule",
    "README",
    "LICENSE"
  ],
  "author": "Ben Parsons <9parsonsb@gmail.com>",
  "license": "MIT",
  "keywords": [
    "angular 15",
    "materialize",
    "materializecss",
    "materialize-css",
    "materialize css"
  ],
  "main": "./lib_commonjs/index.js",
  "module": "./lib_esmodule/index.js",
  "jsnext:main": "./lib_esmodule/index.js",
  "typings": "./lib_commonjs/index.d.ts",
  "scripts": {
    "build:commonjs": "rimraf lib_commonjs && ngc -p tsconfig.json",
    "build:esmodule": "rimraf lib_esmodule && ngc -p tsconfig.module.json",
    "build": "npm run build:commonjs && npm run build:esmodule",
    "prepare": "npm run build",
    "ng": "ng"
  },
  "peerDependencies": {
    "@angular/common": ">=15.0.0",
    "@angular/core": ">=15.0.0"
  },
  "devDependencies": {
    "@angular/animations": "v15-lts",
    "@angular/cli": "v15-lts",
    "@angular/common": "v15-lts",
    "@angular/compiler": "v15-lts",
    "@angular/compiler-cli": "v15-lts",
    "@angular/core": "v15-lts",
    "@angular/forms": "v15-lts",
    "@angular/platform-browser": "v15-lts",
    "@angular/platform-browser-dynamic": "v15-lts",
    "@angular/router": "v15-lts",
    "@materializecss/materialize": "^2.0.3",
    "@types/jquery":"3.5.29",
    "commitizen": "4.2.5",
    "core-js": "^3.25.5",
    "cz-conventional-changelog": "3.3.0",
    "gulp-rimraf": "1.0.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.6.1",
    "materialize-css": "^1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.1.0",
    "run-sequence": "2.2.1",
    "rxjs": "^7.5.7",
    "semantic-release": "^19.0.5",
    "typescript": "~4.8.4",
    "webpack-dev-server": "^4.11.1",
    "zone.js": "0.11.8"
  }
}